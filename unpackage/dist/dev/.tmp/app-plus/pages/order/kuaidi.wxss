.logo {
  position: absolute;
  right: 5%;
  top: 20%;
  width: 120rpx;
  height: 120rpx;
}
.logo image {
  width: 120rpx;
  height: 120rpx;
}
.top-sec {
  position: relative;
}
.empty {
  /*position: fixed;*/
  /*left: 0;*/
  /*top: 0;*/
  width: 100%;
  height: 45vh;
  padding-bottom: 100rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  -webkit-flex-direction: column;
          flex-direction: column;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  background: #fff;
}
.empty image {
    width: 240rpx;
    height: 160rpx;
    margin-bottom: 30rpx;
}
.empty .empty-tips {
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    font-size: 26rpx;
    color: #C0C4CC;
}
.empty .empty-tips .navigator {
      color: #fa436a;
      margin-left: 16rpx;
}
.container {
  width: 100%;
  height: 100%;
  /*margin-top: 130upx;*/
}
.header-container {
  background-color: #fff;
}
.iphone {
  margin-top: 180rpx;
}
page {
  background-color: #f2f2f2;
}
.top-sec {
  background-color: #fff;
  width: 100%;
  margin-top: 20rpx;
  box-sizing: border-box;
  padding: 24rpx 30rpx;
  border-top: 2rpx solid #f5f5f5;
}
.top-sec .a-row {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  font-size: 28rpx;
  line-height: 54rpx;
}
.top-sec .a-row .label {
  color: #999;
  margin-right: 28rpx;
}
.sec-wrap {
  width: 100%;
  background-color: #fff;
  padding-top: 10rpx;
  margin-top: 10rpx;
}
.details-info {
  position: relative;
  width: 100%;
  margin-top: 20rpx;
  overflow: hidden;
}
.details-info ._ul {
  margin: 0;
  padding: 0;
}
.details-info .a-row {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  position: relative;
  z-index: 4;
  width: 93%;
  margin: 0 auto;
  margin-bottom: 14rpx;
  border-bottom: 2rpx dashed #f5f5f5;
  padding-bottom: 10rpx;
}
.details-info .a-row .info {
  font-size: 28rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
}
.date-box {
  font-size: 26rpx;
  color: #333;
  margin-bottom: 20rpx;
  width: 530rpx;
}
.info .text {
  color: #999;
  width: 25%;
  text-align: right;
  font-size: 22rpx;
}
.dot {
  width: 30rpx;
  height: 30rpx;
  margin-right: 30rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
  box-sizing: border-box;
}
.default-dot {
  background: #ddd;
  width: 14rpx;
  height: 14rpx;
  border-radius: 100%;
}
.active-dot {
  border: 2rpx solid #e64340;
  width: 26rpx;
  height: 26rpx;
  border-radius: 50%;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
  background-color: #fff;
}
.active-dot .yuan-red {
  background: #e64340;
  width: 14rpx;
  height: 14rpx;
  border-radius: 50%;
}
.none {
  text-align: center;
  padding-top: 100rpx;
  padding-bottom: 120rpx;
  font-size: 30rpx;
  color: #333;
}

