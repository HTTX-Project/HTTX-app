<view class="container"><view class="l-title"></view><view class="logo"><view class="img"><image mode="widthFix" src="/static/temp/logo.png"></image></view></view><view class="form"><view class="username"><input placeholder="请输入手机号" placeholder-style="color: rgba(255,255,255,0.8);" data-event-opts="{{[['input',[['__set_model',['','phoneNumber','$event',[]]]]]]}}" value="{{phoneNumber}}" bindinput="__e"/></view><view class="password"><input password="true" placeholder="请输入密码" placeholder-style="color: rgba(255,255,255,0.8);" data-event-opts="{{[['input',[['__set_model',['','passwd','$event',[]]]]]]}}" value="{{passwd}}" bindinput="__e"/></view><view data-event-opts="{{[['tap',[['toDoLogin',['$event']]]]]}}" class="btn" bindtap="__e">登 录</view><view class="res"><view data-event-opts="{{[['tap',[['toPage',['register']]]]]}}" bindtap="__e">用户注册</view><view data-event-opts="{{[['tap',[['toPage',['resetpasswd']]]]]}}" bindtap="__e">忘记密码</view></view></view></view>