<view class="container"><view class="carousel-section"><swiper class="carousel" autoplay="true" interval="6000" duration="1000" circular data-event-opts="{{[['change',[['swiperChange',['$event']]]]]}}" bindchange="__e"><block wx:for="{{carouselList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><swiper-item class="carousel-item"><image src="{{url+''+item.PATH}}"></image></swiper-item></block></swiper><view class="swiper-dots"><text class="num">{{swiperCurrent+1}}</text><text class="sign">/</text><text class="num">{{carouselList.length}}</text></view></view><view class="cate-section"><view data-event-opts="{{[['tap',[['goNavTo',['notice']]]]]}}" class="cate-item" bindtap="__e"><image src="/static/temp/homegg.png"></image><text>平台咨询</text></view><view data-event-opts="{{[['tap',[['goNavTo',['sharingInvitations']]]]]}}" class="cate-item" bindtap="__e"><image src="/static/temp/ic1.png"></image><text>邀请新人</text></view><view data-event-opts="{{[['tap',[['goNavTo',['myBill']]]]]}}" class="cate-item" bindtap="__e"><image src="/static/temp/c1.png"></image><text>我的账单</text></view><view data-event-opts="{{[['tap',[['goNavTo',['myIncome']]]]]}}" class="cate-item" bindtap="__e"><image src="/static/temp/homerr.png"></image><text>我的收益</text></view></view><view class="ad-1"><image src="/static/temp/ad1.jpg" mode="scaleToFill"></image></view><view class="f-header m-t"><image src="/static/temp/h1.png"></image><view class="tit-box"><text class="tit">推荐商品</text><text class="tit2">Guess You Like It</text></view></view><view class="hot-floor"><view class="floor-img-box"><image class="floor-img" src="https://timgsa.baidu.com/timg?image&quality=80&size=b9999_10000&sec=1553409398864&di=4a12763adccf229133fb85193b7cc08f&imgtype=0&src=http%3A%2F%2Fb-ssl.duitang.com%2Fuploads%2Fitem%2F201703%2F19%2F20170319150032_MNwmn.jpeg" mode="scaleToFill"></image></view><scroll-view class="floor-list" scroll-x><view class="scoll-wrapper"><block wx:for="{{topGoodsList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['navToDetailPage',['$0'],[[['topGoodsList','',index]]]]]]]}}" class="floor-item" bindtap="__e"><image src="{{url+''+item.PATH}}" mode="aspectFill"></image><text class="title clamp">{{item.NAME}}</text><text class="price">{{"￥"+item.PRICE}}</text></view></block></view></scroll-view></view><view class="f-header m-t"><image src="/static/temp/h1.png"></image><view class="tit-box"><text class="tit">全部商品</text><text class="tit2">All merchandise</text></view></view><view class="guess-section"><block wx:for="{{goodsList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['navToDetailPage',['$0'],[[['goodsList','',index]]]]]]]}}" class="guess-item" bindtap="__e"><view class="image-wrapper"><image src="{{url+''+item.PATH}}" mode="aspectFill"></image></view><text class="title clamp">{{item.NAME}}</text><text class="price">{{"￥"+item.PRICE}}</text></view></block></view></view>